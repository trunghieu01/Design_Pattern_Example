package factory_Pattern;

public class Rectangle implements Bank{
	
	private int longs, width;
	
	public int getLongs() {
		return longs;
	}

	public void setLongs(int longs) {
		this.longs = longs;
	}

	public int getWidth() {
		return width;
	}

	public void setWidth(int width) {
		this.width = width;
	}

	@Override
	public Double perimeter() {
		// TODO Auto-generated method stub
		return (double) ((longs+width)*2);
	}

	@Override
	public Double area() {
		// TODO Auto-generated method stub
		return null;
	}

}
